<!DOCTYPE html>
<html lang="zxx">

<!-- Mirrored from demo.dashboardpack.com/user-management-html/index.html by HTTrack Website Copier/3.x [XR&CO'2014], Wed, 13 Dec 2023 13:29:04 GMT -->

<head>
    {{> header}}
</head>

<body class="crm_body_bg">


    <nav class="sidebar">
        {{> sidebar}}
    </nav>

    <section class="main_content dashboard_part large_header_bg">

        {{> main_header}}

        <div class="main_content_iner overly_inner ">
            <div class="container-fluid p-0 ">

                {{> dashboard_analytics}}
                <div class="row">
                    <div class="col-lg-12">
                        <div class="white_card card_height_100 mb_30 pt-4">
                            <div class="white_card_body">
                                <div class="QA_section">
                                    <div class="white_box_tittle list_header">
                                        <h4>Holiday List </h4>
                                        <div class="box_right d-flex lms_block">
                                            <div class="serach_field_2">
                                                <div class="search_inner">
                                                    <form Active="#">
                                                        <div class="search_field">
                                                            <input type="text" placeholder="Search content here...">
                                                        </div>
                                                        <button type="submit"> <i class="ti-search"></i> </button>
                                                    </form>
                                                </div>
                                            </div>
                                            <div class="add_button ms-2">
                                                <a href="#" data-toggle="modal" data-target="#addcategory"
                                                    class="btn_1">search</a>
                                            </div>
                                        </div>
                                    </div>
                                    <div class="QA_table mb_30">

                                        {{!-- roles --}}
                                        <table class="table lms_table_active ">
                                            <thead>
                                                <tr>
                                                    <th scope="col"><strong>id</strong></th>
                                                    <th scope="col"><strong>Name</strong></th>
                                                    <th scope="col"><strong>Start Date</strong></th>
                                                    <th scope="col"><strong>End Date</strong></th>
                                                    <th scope="col"><strong>Description</strong></th>
                                                    <th scope="col"><strong>Action</strong></th>
                                                </tr>
                                            </thead>
                                            <tbody id="holiday-list">
                                                {{#holidays}}
                                                <tr>
                                                    <td><strong>{{this.count}}</strong></td>
                                                    <td><strong>{{this.name}}</strong></td>
                                                    <td><strong>{{this.startDate}}</strong></td>
                                                    <td><strong>{{this.endDate}}</strong></td>
                                                    <td><strong>"{{this.description}}"</strong></td>
                                                    <td>
                                                        <div class="action_btns d-flex">
                                                            <a href="/holiday/update?_id={{this._id}}"
                                                                class="action_btn mr_10">
                                                                <i class="far fa-edit"></i>
                                                            </a>
                                                            <a style="cursor: pointer;" id="{{this._id}}"
                                                                class="action_btn delete delete-holiday-button"> <i
                                                                    class="fas fa-trash"></i>
                                                            </a>
                                                        </div>
                                                    </td>
                                                </tr>
                                                {{/holidays}}
                                            </tbody>
                                        </table>
                                        {{!-- roles --}}
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>

        {{> main_footer}}
    </section>


    {{> chat_popup_box}}


    <div id="back-top" style="display: none;">
        {{> back_to_top}}
    </div>

    {{> footer}}
</body>

<!-- Mirrored from demo.dashboardpack.com/user-management-html/index.html by HTTrack Website Copier/3.x [XR&CO'2014], Wed, 13 Dec 2023 13:29:54 GMT -->

</html>